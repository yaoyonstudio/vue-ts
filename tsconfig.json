{
  "compilerOptions": {
    "target": "es5",
    "allowSyntheticDefaultImports": true,
    "lib": ["es5", "es2015.promise", "dom"],
    "module": "es2015",
    "moduleResolution": "node",
    "isolatedModules": false,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "removeComments": true,
    "suppressImplicitAnyIndexErrors": true,
    "allowSyntheticDefaultImports": true
  }
}